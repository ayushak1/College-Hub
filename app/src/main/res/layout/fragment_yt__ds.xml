<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/dsasheet">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp"
        android:text="Best Yt Channels"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textAppearance="?textInputOutlinedStyle"
        android:fontFamily="@font/seg_ui_semibold"
        />


    <androidx.cardview.widget.CardView
        android:id="@+id/striverCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/striverTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Take You Forward By Striver"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/loveBabbarCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/striverCardView"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"

        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/loveBabbarTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Love Babbar"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/arshGoyalCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loveBabbarCardView"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/arshGoyalTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Arsh Goyal"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/siddharthSinghCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/arshGoyalCardView"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/white"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/siddharthSinghTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Abdul Bari"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/leadCodingByFrazCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toBottomOf="@id/siddharthSinghCardView"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/leadCodingByFrazTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fraz"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/theCodeSkoolCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/leadCodingByFrazCardView"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/theCodeSkoolTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kunal Kuswaha"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/apnaCollegeCardView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:cardCornerRadius="8dp"
        android:backgroundTint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/theCodeSkoolCardView"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="16dp">

            <TextView
                android:id="@+id/apnaCollegeTitleTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Free Code Camp"
                android:drawableStart="@drawable/ytt"
                android:drawablePadding="10dp"
                android:textSize="20sp"
                android:textColor="@android:color/black"
                android:fontFamily="@font/seg_ui_semibold"
                />
        </LinearLayout>
    </androidx.cardview.widget.CardView>



</androidx.constraintlayout.widget.ConstraintLayout>

